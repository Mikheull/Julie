<!doctype html>
<html lang="fr">
<head>
  <meta charset="utf-8">
  <title>ParisDiscover</title>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
  <link rel="stylesheet" href="/css/result.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>
<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
<body>
        <html>

                <head>
                </head>
                
                <body>
                  <header class="navbar navbar-fixed-top navbar-shadow">
                    <div class="navbar-branding dark">
                      <p class="navbar-brand"><b>Paris Discover</b></p>
                    </div>
                  </header>
                  <aside id="sidebar_left" class="nano nano-light affix has-scrollbar sidebar-default">
                    <div class="sidebar-left-content nano-content" tabindex="0" style="margin-right: -17px;">
                     
                      <div>
                            <form style="color:black;">
                           
                              <div class="field-search">
                                  <input type="text" name="query" required class="search-box" placeholder="Rechercher" />
                              </div>
                                    
                              <div class="field-date">
                                  <input type="date" name="focus" required class="search-box"/>
                              </div>   
                              <div>                      
                              <select name="category" id="pet-select">
                                  <option value="">Catégorie</option>
                                  <option value="dog">Dog</option>
                                  <option value="cat">Cat</option>
                                  <option value="hamster">Hamster</option>                             
                              </select>
                            </div>
                              <div class="field-search">
                                  <input type="text" name="arrondissement" required class="search-box" placeholder="arrondissement" />
                              </div>
                              <div>
                              <select name="access" id="pet-select">
                                  <option value="">type d'accès</option>
                                  <option value="free">gratuit</option>
                                  <option value="paie">payant</option>
                                  <option value="invite">Invitation</option>                             
                              </select>
                            </div>
                              <div>
                                  <label for="scales">Accès PMR</label>
                                  <input type="checkbox" id="scales" name="PMR">
                              </div>
                              <div>
                                    <label for="scales">Malvoyant</label>
                                    <input type="checkbox" id="scales" name="malvoyant">
                              </div>
                              <div>
                                      <label for="scales">Malentendant</label>
                                      <input type="checkbox" id="scales" name="malentendant">
                              </div>
                              <input type="submit" value="Filtrer">
                            </form>
                              
                      </div>
                    </div>
                  </aside>
                </body>
                
                </html>
         <div class="result">
           <% if (result !== 'null') { %>
                <span><%- JSON.stringify(result) %></span>
            <% } %>
        </div>
    
</body>
</html>
